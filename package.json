{
  "name": "backend-keuanganku",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "bun run cf-typegen && wrangler deploy --minify",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 execute keuanganku --local --file=./drizzle/0000_initial.sql",
    "db:migrate:prod": "wrangler d1 execute keuanganku --remote --file=./drizzle/0000_initial.sql",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.7.4",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "firebase-admin": "^13.6.0",
    "hono": "^4.10.6",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "drizzle-kit": "^0.31.7",
    "tsx": "^4.20.6",
    "wrangler": "^4.4.0"
  }
}